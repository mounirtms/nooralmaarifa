# Production Environment Variables
VITE_APP_NAME=Noor Al Maarifa Trading
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Premium Stationery & Office Supplies Dubai UAE

# Build Configuration
VITE_BUILD_TIME=BUILD_TIME_PLACEHOLDER
VITE_NODE_ENV=production

# Performance Settings
VITE_ENABLE_PWA=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_COMPRESSION=true

# Firebase Configuration (Production)
VITE_FIREBASE_API_KEY=your_production_api_key
VITE_FIREBASE_AUTH_DOMAIN=nooralmaarifa.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=nooralmaarifa
VITE_FIREBASE_STORAGE_BUCKET=nooralmaarifa.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# SEO Configuration
VITE_SITE_URL=https://nooralmaarifa.web.app
VITE_SITE_NAME=Noor Al Maarifa Trading L.L.C
VITE_DEFAULT_LOCALE=en
VITE_SUPPORTED_LOCALES=en,ar

# Performance Monitoring
VITE_ENABLE_WEB_VITALS=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Security Settings
VITE_ENABLE_CSP=true
VITE_ENABLE_HSTS=true

# CDN Configuration (if using)
VITE_CDN_URL=
VITE_ASSETS_CDN_URL=

# Third-party Services
VITE_GOOGLE_ANALYTICS_ID=
VITE_GOOGLE_TAG_MANAGER_ID=
VITE_HOTJAR_ID=

# Feature Flags
VITE_ENABLE_ADMIN_PANEL=true
VITE_ENABLE_CONTACT_FORM=true
VITE_ENABLE_GALLERY_UPLOAD=true
VITE_ENABLE_CATALOG_DOWNLOAD=true